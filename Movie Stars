budget = float(input())
command = input()

remuneration = 0

while command != 'ACTION':
    if (len(command)) <= 15:
        remuneration = float(input())
        budget -= remuneration
    elif(len(command)) > 15:
        remuneration = budget * 0.20
        budget -= remuneration

    if budget <= 0:
        break
    command = input()

if budget <= 0:
    print(f"We need {abs(budget):.2f} leva for our actors.")
else:
    print(f"We are left with {abs(budget):.2f} leva.")
